// Replace the whole enterBtn click block with this
$('#enterBtn').addEventListener('click', () => {
  const tour = manifest?.youtube?.url;
  if (tour) {
    try { window.open(tour, '_blank', 'noopener'); } catch (e) { /* popup blocked is fine */ }
  }
  // Always proceed into the experience
  gate.style.display = 'none';
  startPlayback();
});